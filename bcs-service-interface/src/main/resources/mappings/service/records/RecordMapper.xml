<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.phy.bcs.service.records.mapper.RecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.phy.bcs.service.records.model.Record">
        <id column="id" property="id" />
        <result column="type" property="type" />
        <result column="create_time" property="createTime" />
        <result column="dest_sys" property="destSys" />
        <result column="file_name" property="fileName" />
        <result column="path" property="path" />
        <result column="dest_prot" property="destProt" />
        <result column="prot_head" property="protHead" />
        <result column="prot_body" property="protBody" />
        <result column="is_success" property="isSuccess" />
        <result column="success_time" property="successTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id AS id,
        type AS type,
        create_time AS createTime,
        dest_sys AS destSys,
        file_name AS fileName,
        path AS path,
        dest_prot AS destProt,
        prot_head AS protHead,
        prot_body AS protBody,
        is_success AS isSuccess,
        success_time AS successTime
    </sql>
</mapper>
